<head>
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <!-- Include Startr Style CSS -->
  <link href="https://startr.style/style.css" rel="stylesheet">
  <meta name="theme-color" content="#000000">
  <!-- Social Share-->
  <meta property="og:title" content="{{ socialShare.title }}">
  <meta property="og:description" content="{{ socialShare.description }}">
  <meta property="og:image" content="{{ socialShare.social_image }}">
  <meta property="og:url" content="{{ socialShare.url }}">
  <meta name="twitter:card" content="summary_large_image">
  <meta name="twitter:site" content="@startrsh">
  <meta name="twitter:creator" content="@startrsh">
  <meta name="twitter:title" content="{{ socialShare.title }}">
  <meta name="twitter:description" content="{{ socialShare.description }}">
  <meta name="twitter:image" content="{{ socialShare.social_image }}">
  <meta name="twitter:url" content="{{ socialShare.url }}">
  <meta name="description" content="{{ socialShare.description }}">
  <!-- Favicon -->
  <link rel="icon" href="/assets/favicon.ico" type="image/x-icon">
  <!-- Larger icons-->
  <link rel="apple-touch-icon" sizes="180x180" href="/assets/apple-touch-icon.png">
  <link rel="icon" type="image/png" sizes="32x32" href="/assets/favicon-32x32.png">
  <link rel="icon" type="image/png" sizes="16x16" href="/assets/favicon-16x16.png">
  <link rel="manifest" href="/assets/site.webmanifest">
  <!-- Link to social share image -->
  <link rel="image_src" href="{{ socialShare.social_image }}">
  

  <!-- CSS -->
  <style>
    *{max-width: 100%;}

    pre {
      color: rgb(252, 158, 255);
      background: #010;
      font-family: monospace;
      font-size: 12px;
      white-space: pre;
      display: block;
    }

    h1 span,
    h1 b,
    h2 span,
    h2 b,
    h3 span,
    h3 b {
      background: -webkit-linear-gradient(#faf, #470182);
      -webkit-background-clip: text;
      -webkit-text-fill-color: transparent;
    }
  </style>

  <title>
    {{ title or 'Startr.sh'}}
  </title>
  <meta name="description" content="{{ description or 'Startr.sh is simplifying the way you start your next project. We provide you with a simple and easy to use boilerplate to get your project up and running quickly.' }}">
<script type="text/javascript">
  var _paq = window._paq = window._paq || [];
  /* tracker methods like "setCustomDimension" should be called before "trackPageView" */
  _paq.push(['trackPageView']);
  _paq.push(['enableLinkTracking']);
  (function() {
    var u="//visitas.startr.cloud/";
    _paq.push(['setTrackerUrl', u+'matomo.php']);
    _paq.push(['setSiteId', '5']);
    var d=document, g=d.createElement('script'), s=d.getElementsByTagName('script')[0];
    g.type='text/javascript'; g.async=true; g.src=u+'matomo.js'; s.parentNode.insertBefore(g,s);
  })();
</script>
</head>
